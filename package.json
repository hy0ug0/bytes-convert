{
    "name": "bytes-convert",
    "version": "1.0.2",
    "main": "dist/bytes-convert.cjs.js",
    "module": "dist/bytes-convert.esm.js",
    "browser": "dist/bytes-convert.umd.js",
    "license": "GPL-3.0",
    "repository": "git@github.com:hy0ug0/bytes-convert.git",
    "author": "hy0ug0 <hugo.contreras.pro@gmail.com>",
    "keywords": [
        "bytes",
        "octet",
        "convert"
    ],
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "start": "rollup -c -w",
        "build": "rimraf dist/ && rollup -c",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "format": "prettier --write",
        "lint": "eslint \"src/**/*.ts\""
    },
    "devDependencies": {
        "@babel/preset-env": "7.12.1",
        "@babel/preset-typescript": "7.12.1",
        "@types/jest": "26.0.15",
        "@typescript-eslint/eslint-plugin": "4.5.0",
        "@typescript-eslint/parser": "4.5.0",
        "@wessberg/rollup-plugin-ts": "1.3.5",
        "commitizen": "4.2.2",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "7.17.0",
        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-import": "2.22.1",
        "husky": "4.3.0",
        "jest": "26.6.0",
        "lint-staged": "10.4.2",
        "prettier": "2.1.2",
        "rimraf": "3.0.2",
        "rollup": "2.36.0",
        "rollup-plugin-terser": "7.0.2",
        "semantic-release": "17.2.1",
        "typescript": "4.0.3"
    },
    "husky": {
        "hooks": {
            "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.ts": "yarn format"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
